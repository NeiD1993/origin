{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"rules": {
		"no-console": [true, "log"],
		"no-debugger": true,
		"no-eval": true,
		"no-invalid-this": true,
		"no-var-keyword": true,
		"switch-default": true,
		"arrow-return-shorthand": [true],
		"max-classes-per-file": [true, 5],
		"class-name": true,
		"whitespace": [true, "check-branch", "check-operator", "check-typecast"],
		"quotemark": [true, "single"]
	},
	"jsRules": {},
	"rulesDirectory": []
}